<Page x:Class="BankShibaevaAnna322.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BankShibaevaAnna322"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainPage">
    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Page.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Button x:Name="Credits" Content="Кредиты" Grid.Row="0" Width="133" Background="White"
 Click="Credits_Click"/>
        <Button x:Name="Cards" Content="Банковские карты" Grid.Row="1" Width="133" Background="White"
 Click="Cards_Click"/>
        <Button x:Name="Deposits" Content="Вклады" Grid.Row="2" Width="133" Background="White"
 Click="Deposits_Click"/>
        <Button x:Name="Employees" Content="Сотрудники" Grid.Row="3" Width="133" Background="White"
 Click="Employees_Click"/>
        <Button x:Name="Branches" Content="Отделения" Grid.Row="3" Width="133" Background="White"
 Click="Employees_Click"/>
    </Grid>
</Page>

